object = vars()
if (object.ra == "?") {
    line1 = s("a  째 = a * 1째 = a * (pi/180) rad = ") + sl("a*(pi/180)") + " rad"
    return latex([line1])
} else {
    line1 = s("ra rad = ra * 1rad = ra * (180째)/(pi) \\approx ") + sd("ra * (180/pi)") + " 째"
    return latex([line1])
}
